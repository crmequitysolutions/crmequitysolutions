    <p>Property Use Code: <%= @data.use_code %></p>
    <p>Zestimate Price: <%= @data.price %></p>
  <% else %>
    <% if @zillow_pref.links %>
      <% @data.links.each do |name, url| %>
        <p><a href=<%= url %>><% name %></a></p>
      <% end %>
    <% end %>
    <% if @zillow_pref.last_sold_date %>
      <p>Last Sold Date: <%= @data.last_sold_date %></p>
    <% end %>
    <% if @zillow_pref.last_sold_price %>
