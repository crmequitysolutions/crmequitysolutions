<p id="notice"><%= notice %></p>

<h1>Interactions</h1>

<table>
  <thead>
    <tr>
      <th>Interaction</th>
      <th>Contact</th>
      <th>Date time</th>
      <th>Type</th>
      <th>Result</th>
      <th>Note</th>
      <th>Property</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @interactions.each do |interaction| %>
      <tr>
        <td><%= interaction.interaction_id %></td>
        <td><%= interaction.contact_id %></td>
        <td><%= interaction.date_time %></td>
        <td><%= interaction.type %></td>
        <td><%= interaction.result %></td>
        <td><%= interaction.note %></td>
        <td><%= interaction.property_id %></td>
        <td><%= link_to 'Show', interaction %></td>
        <td><%= link_to 'Edit', edit_interaction_path(interaction) %></td>
        <td><%= link_to 'Destroy', interaction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Interaction', new_interaction_path %>
